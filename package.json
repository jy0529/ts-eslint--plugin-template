{
  "name": "eslint-plugin-test",
  "version": "0.0.1",
  "description": "ts eslint plugin template",
  "main": "lib/",
  "files": [
    "docs/",
    "lib/"
  ],
  "scripts": {
    "build": "babel --extensions .js,.ts src --out-dir lib --copy-files && rimraf lib/__tests__ 'lib/**/__tests__'",
    "test": "jest",
    "typecheck": "tsc -p .",
    "postinstall": "husky install"
  },
  "repository": {
    "url": "https://github.com/jy0529/ts-eslint--plugin-template"
  },
  "keywords": [
    "typescript",
    "eslint",
    "eslint plugin",
    "eslint-plugin",
    "eslint-plugin-template"
  ],
  "author": "Jy <1041207253@qq.com>",
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "projects": [
      {
        "displayName": "test",
        "testPathIgnorePatterns": [
          "<rootDir>/lib/.*",
          "<rootDir>/src/rules/__tests__/fixtures/*",
          "<rootDir>/src/rules/__tests__/test-utils.ts"
        ]
      }
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-jest": "^28.1.1",
    "eslint": "^8.18.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.30.0"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  }
}
